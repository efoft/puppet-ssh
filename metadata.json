{
        "name": "efoft-ssh",
        "version": "1.0.1",
        "author": "efoft",
        "license": "Apache-2.0",
        "source": "https://github.com/efoft/puppet-ssh.git",
        "dependencies": [
                { "name": "puppetlabs/stdlib", "version_requirement":">=4.25.1 < 5.0.0" }
        ],
        "data_provider": "hiera",
        "operatingsystem_support": [
                {
                  "operatingsystem": "RedHat",
                  "operatingsystemrelease": [
                    "6",
                    "7"
                  ]
                },
                {
                  "operatingsystem": "CentOS",
                  "operatingsystemrelease": [
                    "6",
                    "7"
                  ]
                }
        ],
        "requirements": [
          {
            "name": "puppet",
            "version_requirement": ">= 4.5.0 < 5.0.0"
          }
        ],
        "description": "Installs latest ssh server and client packages and configures the server."
}
